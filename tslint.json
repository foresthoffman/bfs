{
	"extends": "tslint:recommended",
	"rules": {
		"indent": false,
		"max-line-length": {
			"options": [
				120
			]
		},
		"new-parens": true,
		"no-unsafe-finally": false,
		"prefer-for-of": false,
		"radix": false,
		"max-classes-per-file": false,
		"no-shadowed-variable": false,
		"no-arg": true,
		"no-bitwise": true,
		"no-conditional-assignment": true,
		"no-consecutive-blank-lines": false,
		"no-console": {
			"severity": "warning",
			"options": [
				"debug",
				"info",
				"log",
				"time",
				"timeEnd",
				"trace"
			]
		}
	},
	"jsRules": {
		"max-line-length": {
			"options": [
				120
			]
		}
	}
}